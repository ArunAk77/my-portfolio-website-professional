{
    "github": {
      "silent": true
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  